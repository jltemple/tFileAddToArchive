<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.1"
    STATUS="ALPHA"
    STARTABLE="true"
    COMPATIBILITY="ALL"

    AUTHOR="Joshua Luke Temple"
    RELEASE_DATE="20151223A"
  >
    <SIGNATURE/>
  </HEADER>
  
  <FAMILIES>
    <FAMILY>File/Management</FAMILY>
  </FAMILIES>
  
  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>
  
  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_OUTPUT="0" MAX_INPUT="0"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK" />
    <CONNECTOR CTYPE="COMPONENT_ERROR" />
    <CONNECTOR CTYPE="RUN_IF" />
  </CONNECTORS>
  
  <PARAMETERS>
    <PARAMETER 
      NAME="FILENAME" 
      FIELD="FILE" 
      REQUIRED="true" 
      NUM_ROW="3" 
      >
      <DEFAULT>"in.txt"</DEFAULT>
    </PARAMETER>

    <PARAMETER 
      NAME="FILEPATH" 
      FIELD="FILE" 
      REQUIRED="true" 
      NUM_ROW="1" 
      >
      <DEFAULT>"__COMP_DEFAULT_FILE_DIR__/in.txt"</DEFAULT>
    </PARAMETER>

    <PARAMETER 
      NAME="ZIPFILE" 
      FIELD="FILE" 
      REQUIRED="true" 
      NUM_ROW="2"
      >
      <DEFAULT>"__COMP_DEFAULT_FILE_DIR__/archive.zip"</DEFAULT>
    </PARAMETER>
  </PARAMETERS>
  
  <CODEGENERATION/>
  
  <RETURNS>
    <RETURN NAME="ARCHIVE_FILEPATH" TYPE="id_String" AVAILABILITY="AFTER" />
    <RETURN NAME="ARCHIVE_FILENAME" TYPE="id_String" AVAILABILITY="AFTER" />
  </RETURNS>
  
</COMPONENT>